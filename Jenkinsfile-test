pipeline {
    agent {label 'ansible'}
    environment {
        ANSIBLE_KEY=credentials('ANSIBLE_KEY')
    }
    stages {
        stage('Versions'){
            steps {
                sh '''
                    ansible --version
                    ansible-playbook --version
                    ansible-galaxy --version
                    ansible-lint --version
                '''
            }
        }
    }
}

def secrets = [
    [path: 'secret/jenkins/ad_pass', secretValues: [
            [vaultKey: 'username'],
            [vaultKey: 'password']
            ]
        ]
    ]